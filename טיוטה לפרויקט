   "source": [
    "import os\n",
    "def split_file(input_file, output_file1, output_file2):\n",
    "    # בדוק אם הקובץ קיים\n",
    "    if not os.path.exists(input_file):\n",
    "        print(f\"File {input_file} does not exist.\")\n",
    "        return\n",
    "\n",
    "    # קרא את כל השורות בקובץ\n",
    "    with open(input_file, 'r', encoding='unicode_escape') as file:\n",
    "        lines = file.readlines()\n",
    "\n",
    "    # חשב את הנקודה שבה נחלק את הקובץ\n",
    "    half = len(lines) // 2\n",
    "\n",
    "    # כתוב את החלק הראשון\n",
    "    with open(output_file1, 'w', encoding='unicode_escape') as file:\n",
    "        file.writelines(lines[:half])\n",
    "\n",
    "    # כתוב את החלק השני\n",
    "    with open(output_file2, 'w', encoding='unicode_escape') as file:\n",
    "        file.writelines(lines[half:])\n",
    "\n",
    "    print(f\"File {input_file} was split into {output_file1} and {output_file2}.\")\n",
    "\n",
    "# הגדר את נתיבי הקבצים\n",
    "input_file = r\"C:\\Users\\user\\OneDrive\\מסמכים\\Python Scripts\\sharck attack.csv\"\n",
    "output_file1 = r\"C:\\Users\\user\\OneDrive\\מסמכים\\Python Scripts\\sharck_attack_part1.csv\"\n",
    "output_file2 = r\"C:\\Users\\user\\OneDrive\\מסמכים\\Python Scripts\\sharck_attack_part2.csv\"\n",
    "\n",
    "# פצל את הקובץ\n",
    "split_file(input_file, output_file1, output_file2)"
   ]
